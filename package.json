{
  "name": "housepro",
  "version": "1.0.0",
  "description": "House Pro Digital",
  "author": "leonachan1 <1nomadproductions@gmail.com>",
  "private": true,
  "scripts": {
    "client": "cross-env webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "client:dashboard": "cross-env CLIENT_ENV=dashboard webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "server": "cross-env NODE_ENV=development nodemon server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:dashboard": "concurrently \"npm run server\" \"npm run client:dashboard\"",
    "e2e:local": "cypress open --env baseUrl=http://localhost:8080/",
    "e2e:test": "cypress run --env baseUrl=https://houseproapp-test.herokuapp.com/",
    "e2e:test:open": "cypress open --env baseUrl=https://houseproapp-test.herokuapp.com/",
    "e2e:stage": "cypress run --env baseUrl=https://houseproapp-stage.herokuapp.com/",
    "e2e:stage:open": "cypress open --env baseUrl=https://houseproapp-stage.herokuapp.com/",
    "e2e:prod": "cypress run --env baseUrl=https://houseproapp-prod.herokuapp.com/",
    "e2e:prod:open": "cypress open --env baseUrl=https://houseproapp-prod.herokuapp.com/",
    "postinstall": "npm run build && npm run build:dashboard",
    "start": "node server/server.js",
    "test": "npm run test:src && npm run test:dashboard && npm run test:server",
    "test:src": "cross-env NODE_ENV=test jest --config=src/jest.config.js",
    "test:dashboard": "cross-env NODE_ENV=test jest --config=dashboard/jest.config.js",
    "test:server": "cross-env NODE_ENV=test jest --config=server/jest.config.js --detectOpenHandles",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "build": "node build/build.js",
    "build:dashboard": "cross-env CLIENT_ENV=dashboard node build/build.js"
  },
